#include<iostream>
using namespace std;
int main()
{
	long long t;
	cin>>t;
	while(t--)
		{
			long long n,s=0,m,p=0,q=0,r,i,j,k,l;
			cin>>n;
			long long a[n],b[n];
			for(i=0;i<n;i++)
			{
				cin>>a[i];
			}
			for(i=0;i<n;i++)
			{
				cin>>b[i];
			}
			
			for(i=0;i<n;i++)
			{
				s=a[i]*20-b[i]*10;
				if(s<0)
				{
					s=0;
				}
				if(s>p)
				{
					p=s;
				}
				s=0;
			}
			cout<<p<<endl;
		}
	
	
}
